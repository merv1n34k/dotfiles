{
    "name": "snips",
    "engines": {
        "vscode": "^1.11.0"
    },
    "contributes": {
        "snippets": [
            {
                "language": [
                    "markdown",
                    "tex",
                    "html",
                ],
                "path": "./snippets/global.json"
            },
            {
                "language": "markdown",
                "path": "./snippets/markdown.json"
            },
            {
                "language": [
                    "json"
                ],
                "path": "./snippets/json.json"
            },
            {
                "language": ["css", "scss"],
                "path": "./snippets/css.json"
            },
            {
                "language": "tex",
                "path": "./snippets/tex.json"
            }
    ]
    }
}
